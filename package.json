  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/aos": "^3.0.7",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
